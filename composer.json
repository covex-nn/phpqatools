{
    "name" : "covex-nn/phpqatools",
    "type" : "library",
    "description" : "Composer package for PHP QA Tools",
    "license" : "MIT",
    "require" : {
        "php" : ">=5.5.9"
    },
    "suggest" : {
        "apnet/coding-standard" : "Apnet PSR-2 PHP coding standard"
    },
    "extra" : {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload" : {
        "psr-4" : { "JooS\\Jenkins\\": "src/JooS/Jenkins/" }
    },
    "bin" : [
        "bin/pdepend.phar",
        "bin/phpcb.phar",
        "bin/phpcpd.phar",
        "bin/phpcs.phar",
        "bin/phpdoc.phar",
        "bin/phploc.phar",
        "bin/phpmd.phar",
        "bin/phpunit.phar"
    ]
}
