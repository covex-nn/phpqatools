{
    "name" : "covex-nn/phpqatools",
    "type" : "library",
    "description" : "Composer package for PHP QA Tools",
    "license" : "MIT",
    "suggest" : {
        "apnet/coding-standard" : "Apnet PSR-2 PHP coding standard"
    },
    "extra" : {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload" : {
        "psr-4" : { "JooS\\Jenkins\\": "src/JooS/Jenkins/" }
    },
    "bin" : [
        "pdepend.phar",
        "phpcb.phar",
        "phpcpd.phar",
        "phpcs.phar",
        "phpdoc.phar",
        "phploc.phar",
        "phpmd.phar",
        "phpunit.phar"
    ]
}
