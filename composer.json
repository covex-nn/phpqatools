{
    "name" : "covex-nn/phpqatools",
    "type" : "library",
    "description" : "Composer package for PHP QA Tools",
    "license" : "MIT",
    "require" : {
        "pdepend/pdepend" : "~1.1",
        "phpmd/phpmd" : "~1.5",
        "sebastian/phpcpd" : "~2.0",
        "squizlabs/php_codesniffer" : "~1.5",
        "covex-nn/phpcb": "~1.0",
        "phpunit/phpunit" : ">=3.7.0",
        "phploc/phploc" : "~2.0",
        "phpdocumentor/phpdocumentor": "~2.4"
    },
    "suggest" : {
        "apnet/coding-standard" : "Apnet PHP coding standard"
    },
    "extra" : {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload" : {
        "psr-0" : { "JooS\\Jenkins\\": "src/" }
    },
    "config": {
        "bin-dir" : "bin",
        "github-oauth" : {
        }
    }
}
