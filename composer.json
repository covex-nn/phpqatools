{
    "name" : "covex-nn/phpqatools",
    "type" : "library",
    "description" : "Composer package for PHP QA Tools",
    "license" : "MIT",
    "require" : {
        "pdepend/pdepend" : "~2.0",
        "phpmd/phpmd" : "~2.2",
        "sebastian/phpcpd" : "~2.0",
        "squizlabs/php_codesniffer" : "~2.0",
        "covex-nn/phpcb": "~1.0",
        "phpunit/phpunit" : "~4.0",
        "phploc/phploc" : "~2.0",
        "phpdocumentor/phpdocumentor": "~2.8"
    },
    "suggest" : {
        "apnet/coding-standard" : "Apnet PSR-2 PHP coding standard"
    },
    "extra" : {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload" : {
        "psr-4" : { "JooS\\Jenkins\\": "src/JooS/Jenkins/" }
    },
    "config": {
        "bin-dir" : "bin"
    }
}
